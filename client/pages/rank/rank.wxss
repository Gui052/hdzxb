@import "../../utils/base_bg.wxss";

.tab {
  background:transparent;
  display: flex;
  justify-content: space-around;
  text-align: center;
  font-size: 32rpx;
  line-height: 80rpx;
  color: #999;
  font-weight: bold;
}

.tab view {
  width: 30%;
}

.on {
  color: #333;
  border-bottom: 4rpx solid #338fff;
}

.item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  width: 80%;
  margin: 30rpx auto;
  border-radius: 10rpx;
  padding: 20rpx;
  background: #999;
  background: url('data:image/jpeg;base64,') no-repeat;
}

.item image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 8rpx solid #fff;
}

.itemDetail {
  display: flex;
  justify-content: space-between;
}

.itemDetail text {
  display: inline-block;
  margin: 10rpx;
}




.backgroundViewStyle{
  margin-top:30rpx;background:white;height:500rpx;flex-direction:column;border-top:1rpx solid #e5e5e5;
}
.demoButtonStyle{
  background-color:#3CB371;width:60%;height:100rpx;color:#fff;
}

.dialog_mask {
  position: fixed;top: 0;left: 0;right: 0;bottom: 0;z-index: 10;background: rgba(0, 0, 0, 0.5);display: none;
}
.dialog_container {
  position: fixed;bottom: 0;width: 750rpx;background: white;transform: translateY(150%);transition: all 0.4s ease;z-index: 11;
}
.dialog_show .dialog_container {
  transform: translateY(0);
}
.dialog_show .dialog_mask {
  display: block;
}

.dialog__mask {
  position: fixed;top: 0;left: 0;right: 0;bottom: 0;z-index: 10;background: rgba(0, 0, 0, 0.5);display: none;
}
.dialog__container {
  position: fixed;bottom: 0;width: 750rpx;transform: translateY(150%);transition: all 1.5s ease;z-index: 11;height:100%;
}
.dialog--show .dialog__container {
  transform: translateY(0);
}
.dialog--show .dialog__mask {
  display: block;
}
.trophy {
  width: 80%;
  height: 50%;
}